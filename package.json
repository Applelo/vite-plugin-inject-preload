{
  "name": "vite-plugin-inject-preload",
  "version": "1.3.2",
  "packageManager": "pnpm@8.6.0",
  "description": "A Vite plugin for injecting <link rel='preload'>",
  "author": "Applelo",
  "license": "GPL-3.0",
  "homepage": "https://github.com/Applelo/vite-plugin-inject-preload",
  "repository": {
    "type": "git",
    "url": "https://github.com/Applelo/vite-plugin-inject-preload"
  },
  "bugs": "https://github.com/Applelo/vite-plugin-inject-preload/issues",
  "keywords": [
    "html",
    "link",
    "preload",
    "inject",
    "vite",
    "vite-plugin"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14.18.0"
  },
  "scripts": {
    "format": "prettier --write --cache .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "vite": "^3.0.0 || ^4.0.0"
  },
  "dependencies": {
    "mime-types": "^2.1.35"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.40.0",
    "@types/mime-types": "^2.1.1",
    "@vitest/coverage-v8": "^0.34.1",
    "c8": "^8.0.1",
    "eslint": "^8.46.0",
    "rollup": "^3.27.2",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.8",
    "vitest": "^0.34.1"
  }
}
