{
  "name": "vite-plugin-inject-preload",
  "version": "1.3.1",
  "description": "A Vite plugin for injecting <link rel='preload'>",
  "author": "Applelo",
  "license": "GPL-3.0",
  "homepage": "https://github.com/Applelo/vite-plugin-inject-preload",
  "repository": {
    "type": "git",
    "url": "https://github.com/Applelo/vite-plugin-inject-preload"
  },
  "bugs": "https://github.com/Applelo/vite-plugin-inject-preload/issues",
  "keywords": [
    "html",
    "link",
    "preload",
    "inject",
    "vite",
    "vite-plugin"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14.18.0"
  },
  "scripts": {
    "format": "prettier --write --cache .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "coverage": "vitest run --coverage"
  },
  "peerDependencies": {
    "vite": "^3.0.0 || ^4.0.0"
  },
  "dependencies": {
    "mime-types": "^2.1.35"
  },
  "devDependencies": {
    "@types/mime-types": "^2.1.1",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "@vitest/coverage-c8": "^0.29.1",
    "c8": "^7.13.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-define-config": "^1.15.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "prettier": "^2.8.4",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vitest": "^0.29.1"
  },
  "packageManager": "pnpm@7.3.0"
}
